{
  "name": "flixpatrol-top10",
  "version": "1.2.2",
  "description": "Get top10 list from flixpatrol and upload them to trakt",
  "main": "app.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/app.js",
    "start:dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "npm run build && rm -rf bin/ &&  npx ncc build build/app.js -o build/tmp && npx pkg build/tmp/index.js --targets=node18-linux-x64,node18-macos-x64,node18-win-x64 -o bin/flixpatrol-top10"
  },
  "keywords": [
    "Flixpatrol",
    "Trakt",
    "Top10"
  ],
  "author": "Navino16",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/config": "^3.3.1",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "config": "^3.3.9",
    "jsdom": "^22.1.0",
    "trakt.tv": "^8.2.0",
    "winston": "^3.10.0"
  }
}
