{
  "name": "flixpatrol-top10",
  "version": "2.13.1",
  "description": "Get top10 list from flixpatrol and upload them to trakt",
  "main": "app.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/app.js",
    "start:dev": "nodemon",
    "lint": "eslint .",
    "lint-and-fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "package": "npm run build && rm -rf bin/ && mkdir -p bin/ && npx ncc build build/app.js -o build/tmp && npx pkg --config=package.json build/tmp/index.js && mv bin/flixpatrol-top10-linux-x64 bin/flixpatrol-top10-linux-amd64"
  },
  "keywords": [
    "Flixpatrol",
    "Trakt",
    "Top10"
  ],
  "author": "Navino16",
  "license": "GPL-3.0",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/config": "^3.3.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.2.2",
    "@vercel/ncc": "^0.38.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@yao-pkg/pkg": "^6.12.0",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "axios-retry": "^4.5.0",
    "config": "^4.2.0",
    "file-system-cache": "^2.4.7",
    "jsdom": "^27.4.0",
    "trakt.tv": "^8.2.0",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "pkg": {
    "assets": "build/tmp/*.css",
    "targets": [
      "node22-linux-x64",
      "node22-linux-arm64",
      "node22-macos-x64",
      "node22-win-x64"
    ],
    "outputPath": "bin"
  }
}
